    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.7.2",
    "npm-run-all2": "^6.1.2",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
